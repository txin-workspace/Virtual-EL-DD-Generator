          {
            "type": "string",
            "enum": [
              "undefined"
            ],
            "values": [
              {
                "value": "undefined",
                "descriptions": {
                  "ja": "未設定",
                  "en": "Undefined"
                },
                "edt": "0xFFFFFFFF"
              }
            ]
          }
