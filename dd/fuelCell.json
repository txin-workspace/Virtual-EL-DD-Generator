      "observable": false,
      "schema": {
        "oneOf": [
          {
            "type": "object",
            "properties": {
              "startTime": {
                "descriptions": {
                  "ja": "開始時刻",
                  "en": "Start time"
                },
                "type": "string",
                "format": "time"
              },
              "endTime": {
                "descriptions": {
                  "ja": "終了時刻",
                  "en": "End time"
                },
                "type": "string",
                "format": "time"
              }
