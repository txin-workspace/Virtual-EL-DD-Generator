            "edt": "0x00"
          },
          {
            "value": "independent",
            "descriptions": {
              "ja": "独立",
              "en": "Independent type"
            },
            "edt": "0x01"
          },
          {
            "value": "reversePowerFlowNotAcceptable",
            "descriptions": {
              "ja": "系統連系 (逆潮流不可)",
              "en": "System interconnected type (reverse power flow not acceptable)"
            },
            "edt": "0x02"
