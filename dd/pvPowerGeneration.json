            "oneOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "string",
                "enum": [
                  "unknown"
                ],
                "values": [
                  {
                    "value": "unknown",
                    "descriptions": {
                      "ja": "不明",
                      "en": "Unknown"
                    },
                    "edt": "0xFFFFFFFF"
                  }
                ]
              }
            ]
          },
          "intervalTime": {
            "type": "number",
            "unit": "minutes",
            "minimum": 0,
            "maximum": 1800,
            "multipleOf": 30
          },
          "powerControlRatio": {
            "descriptions": {
              "ja": "0x00～0x64 (0～100%) x 96 (30分毎の値、2日分)",
              "en": "0x00-0x64 (0-100%) x 96 (values every 30 minutes, for 2 days)"
